<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"

             xmlns:views="clr-namespace:Waveify.Views"
             xmlns:controls="clr-namespace:Waveify.CustomControls"
             xmlns:viewModels="clr-namespace:Waveify.ViewModel">

    <DataTemplate DataType="{x:Type viewModels:HomeVM}">
        <views:HomeView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:PlaylistVM}">
        <views:PlaylistView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:DownloadsVM}">
        <views:SearchView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:MyFileVM}">
        <views:MyFileView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:PlayerVM}">
        <controls:MusicPanelControl/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:ToolbarVM}">
        <controls:ToolbarControl />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:SettingVM}">
        <views:SettingsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:ProfileVM}">
        <views:ProfileView/>
    </DataTemplate>


</ResourceDictionary>